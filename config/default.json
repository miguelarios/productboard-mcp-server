{
  "server": {
    "port": 3000,
    "host": "localhost",
    "timeout": 30000
  },
  "api": {
    "baseUrl": "https://api.productboard.com",
    "timeout": 10000,
    "retryAttempts": 3,
    "retryDelay": 1000
  },
  "rateLimit": {
    "global": 100,
    "windowMs": 60000,
    "perTool": {
      "pb_feature_create": 50,
      "pb_feature_update": 50,
      "pb_feature_delete": 20,
      "pb_note_create": 100
    }
  },
  "cache": {
    "enabled": true,
    "ttl": 300,
    "maxSize": 100
  },
  "sampling": {
    "temperature": 0.7,
    "max_tokens": 4096,
    "top_p": 0.95,
    "top_k": 50,
    "stop_sequences": []
  },
  "resources": {
    "enabled": true,
    "refreshInterval": 300
  },
  "prompts": {
    "enabled": true,
    "templatesPath": "./prompts"
  },
  "logLevel": "info",
  "logPretty": true
}